{
  "name": "algo-with-nandax",
  "version": "1.0.0",
  "private": true,
  "description": "AI-Powered Algorithmic Trading SaaS Platform with Zerodha Kite Integration",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --parallel --filter './apps/*' dev",
    "dev:api": "pnpm --filter @algo-nandax/api dev",
    "dev:web": "pnpm --filter @algo-nandax/web dev",
    "build": "pnpm --filter './apps/*' build",
    "build:api": "pnpm --filter @algo-nandax/api build",
    "build:web": "pnpm --filter @algo-nandax/web build",
    "lint": "pnpm --filter './apps/*' lint",
    "test": "pnpm --filter './apps/*' test",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "setup": "bash scripts/setup.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.0",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "algorithmic-trading",
    "ai-trading",
    "zerodha-kite",
    "trading-platform",
    "saas",
    "nestjs",
    "nextjs",
    "typescript"
  ],
  "author": "NandaX Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/AmanVatsSharma/algo-with-nandax"
  }
}
